{
  "name": "word hunter",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "find word",
  "permissions": ["tabs", "contextMenus", "http: // */*", "https: // */*"],
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },

  "background": {
    "scripts": ["js/background.js"]
  }
}
